@page
@model CarProject.ConfirmModel
@{
    ViewData["Title"] = "Confirm";
}



    <div class="container text-center">
        <h1 class="text-center">Confirm</h1>

        <p>Thank you for order.</p>
        

        @if (TempData.Peek("Message") != null) {
            <p>Your order number is @TempData.Peek("Message")</p>
        }

        <a class="nav-link text-primary btn btn-outline-primary m-1" asp-area="" asp-page="../Index">Go to homepage</a>
    </div>
