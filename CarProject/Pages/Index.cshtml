@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div class="container">
    <form class="form" method="post">
        <div asp-validation-summary="All"></div>

        <div class="form-group">
            <label asp-for="Inquiry.StartDate" class="col-md-3 right">Start:</label>
            <input asp-for="Inquiry.StartDate" class="form-control" />
            <span asp-validation-for="Inquiry.StartDate"></span>
        </div>



        <div class="form-group">
            <label asp-for="Inquiry.EndDate" class="col-md-3 right">End:</label>
            <input asp-for="Inquiry.EndDate" class="form-control" />
            <span asp-validation-for="Inquiry.EndDate"></span>
        </div>

        <button type="submit" class="btn btn-primary btn-lg btn-block mt-5">Send</button>
    </form>
</div>